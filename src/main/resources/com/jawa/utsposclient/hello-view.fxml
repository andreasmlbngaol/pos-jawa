<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawa.utsposclient.LoginController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TextArea fx:id="statusTextArea" disable="true" prefHeight="100.0" prefWidth="100.0" />

  <Label text="Name:" />
   <TextField fx:id="usernameTextField" />
   <Label text="Password:" />
   <TextField fx:id="passwordTextField" />
  <Button onAction="#onLoginButtonClick" prefHeight="19.0" prefWidth="66.0" text="Login" />
   <Button onAction="#onHelloButtonClick" prefHeight="19.0" prefWidth="66.0" text="Hello" />
   <Button onAction="#onClearSession" text="Clear Session" />
</VBox>
