<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawa.utsposclient.views.SetPasswordController">
    <VBox prefHeight="400.0" prefWidth="600.0">
        <Label text="%password_label"/>
        <PasswordField fx:id="password"/>
        <Label text="%confirm_password_label"/>
        <PasswordField fx:id="confirmPassword"/>
        <Button mnemonicParsing="false" onAction="#onSetPassword" text="%save_button"/>
    </VBox>
</AnchorPane>
