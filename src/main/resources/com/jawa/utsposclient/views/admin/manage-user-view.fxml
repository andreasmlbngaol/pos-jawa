<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawa.utsposclient.views.admin.ManageUserController">
    <VBox prefHeight="400.0" prefWidth="600.0">
        <Button mnemonicParsing="false" onAction="#onBackToHome" text="%back_button"/>
        <TableView fx:id="userTable" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="userIdColumn" prefWidth="103.0" text="%user_id_header"/>
                <TableColumn fx:id="usernameColumn" prefWidth="121.0" text="%username_header"/>
                <TableColumn fx:id="nameColumn" prefWidth="121.0" text="%name_header"/>
                <TableColumn fx:id="roleColumn" prefWidth="110.0" text="%role_header"/>
                <TableColumn fx:id="actionColumn" prefWidth="75.0" text="%action_header"/>
            </columns>
        </TableView>
        <Button mnemonicParsing="false" onAction="#onShowAddCashierDialog" text="%show_add_cashier_dialog_button"/>
    </VBox>
</AnchorPane>
