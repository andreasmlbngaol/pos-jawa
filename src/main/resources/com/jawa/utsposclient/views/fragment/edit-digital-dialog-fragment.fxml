<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<DialogPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0A061E;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawa.utsposclient.views.fragment.EditDigitalDialogController">
    <content>
        <AnchorPane>
            <VBox prefHeight="400.0" prefWidth="607.0">
                <VBox>
                    <Label text="%sku_label" textFill="WHITE">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" top="20.0" />
                  </VBox.margin>
                  <font>
                     <Font size="14.0" />
                  </font></Label>
                    <TextField fx:id="skuTextField" disable="true" editable="false" promptText="%sku_label">
                  <VBox.margin>
                     <Insets bottom="20.0" left="20.0" />
                  </VBox.margin></TextField>
                </VBox>
                <VBox layoutX="10.0" layoutY="10.0">
                    <Label text="%name_label" textFill="WHITE">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" />
                  </VBox.margin>
                  <font>
                     <Font size="14.0" />
                  </font></Label>
                    <TextField fx:id="nameTextField" promptText="%name_label">
                  <VBox.margin>
                     <Insets bottom="20.0" left="20.0" />
                  </VBox.margin></TextField>
                </VBox>
                <VBox layoutX="10.0" layoutY="53.0">
                    <Label text="%price_label" textFill="WHITE">
                  <VBox.margin>
                     <Insets left="20.0" />
                  </VBox.margin>
                  <font>
                     <Font size="14.0" />
                  </font></Label>
                    <TextField fx:id="priceTextField" promptText="%price_label">
                  <VBox.margin>
                     <Insets bottom="20.0" left="20.0" top="10.0" />
                  </VBox.margin></TextField>
                </VBox>
                <VBox layoutX="10.0" layoutY="96.0">
                    <Label text="%url_label" textFill="WHITE">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" />
                  </VBox.margin>
                  <font>
                     <Font size="14.0" />
                  </font></Label>
                    <TextField fx:id="urlTextField" promptText="%url_label">
                  <VBox.margin>
                     <Insets bottom="20.0" left="20.0" />
                  </VBox.margin></TextField>
                </VBox>
                <VBox layoutX="10.0" layoutY="140.0">
                    <Label text="%vendor_name_label" textFill="WHITE">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" />
                  </VBox.margin>
                  <font>
                     <Font size="14.0" />
                  </font></Label>
                    <TextField fx:id="vendorTextField" promptText="%vendor_name_label">
                  <VBox.margin>
                     <Insets left="20.0" />
                  </VBox.margin></TextField>
                </VBox>
            </VBox>
        </AnchorPane>
    </content>
    <ButtonType buttonData="OK_DONE" text="%save_label" />
    <ButtonType buttonData="CANCEL_CLOSE" text="%cancel_label" />
</DialogPane>
